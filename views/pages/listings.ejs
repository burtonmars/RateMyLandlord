<!DOCTYPE HTML>
<html>

<head>
  <meta charset="utf-8">
  <%- include ('../partials/styling.ejs') %>
</head>

<body>
  <%- include ('../partials/top-nav.ejs') %>

  <div class="apartment-bg-image-listings">
    
    <div class="container" id="listings-container-outer">
      
      <h1>Rental Listings</h1>

      <section id="matching-listing" class="container">
          <% for (var i = 0; i < listings.length; i++) { %>
          <h3><%= listings[i].listingTitle %></h3>
          <h4><%= listings[i].listingArea %></h4>
          <p><%= listings[i].listingDescription %></p>
          <div class="specs">
            <p>Bedrooms: <%= listings[i].listingBedrooms %></p>
            <p>Bathrooms: <%= listings[i].listingBathrooms %></p>
          </div>
          <ul>
            <li>Price: <%= listings[i].listingPrice %></li>
            <li><a href="#" class="btn btn-reverse">Contact Landlord</a></li>
          </ul>
          <br>
          <% } %>
      </section>


    </div>
  
  </div>


  <%- include ('../partials/footer.ejs') %>
</body>